{
	"name": "tacoberu/nette-mailing",
	"type": "library",
	"description": "Support send mail in nette application.",
	"keywords": ["mailer", "nette"],
	"homepage": "https://github.com/tacoberu/nette-mailer",
	"license": "MIT",
	"authors": [
		{ "name": "Martin Takáč", "homepage": "http://martin.takac.name", "email": "martin@takac.name" }
	],
	"support": {
		"issues": "https://github.com/tacoberu/nette-mailer/issues"
	},
	"require": {
		"php": ">=7.2 <8.4",
		"nette/mail": "^2",
		"nette/utils": "^2"
	},
	"require-dev": {
		"tracy/tracy": "^2.3",
		"phpunit/phpunit": "^4",
		"phpmd/phpmd": "^2.5",
		"latte/latte": "^2.4",
		"nette/application": "^2.4"
	},
	"autoload": {
		"psr-4": {"Taco\\Nette\\Mailing\\": "libs/"},
		"classmap": ["libs/interfaces.php"]
	},
	"autoload-dev": {
		"psr-4": {
			"Taco\\Nette\\Mailing\\": "tests"
		}
	}
}
